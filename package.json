{
  "name": "nightbear",
  "version": "0.0.1",
  "author": "Marja KÃ¤pyaho <marja.kapyaho@iki.fi>",
  "contributors": [
    "Jarno Rantanen <jarno@jrw.fi>"
  ],
  "license": "AGPL",
  "engines": {
    "node": "16.13.2",
    "npm": "8.1.2"
  },
  "scripts": {
    "\n==================== GLOBAL HOOKS ====================": "",
    "preinstall": "if [ -f ./node_modules/.bin/check-node-version ]; then check-node-version --package; fi",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run lint && npm test",
    "\n==================== GLOBAL QA ====================": "",
    "test": "NODE_PATH=src TS_NODE_PROJECT=tsconfig.mocha.json mocha --require ts-node/register --require $(find src -name PouchDb.mocha.ts) $(npm run --silent list-tests)",
    "lint": "concurrently npm:lint-node npm:lint-tsc npm:lint-eslint npm:lint-prettier",
    "lint-node": "check-node-version --package",
    "lint-tsc": "tsc --project . --noEmit",
    "lint-eslint": "eslint --max-warnings 0 $(npm run --silent list-ts)",
    "lint-prettier": "prettier --list-different $(npm run --silent list-ts) $(npm run --silent list-scss)",
    "list-ts": "find src -name '*.ts*' | grep -v '.snap$' | xargs echo",
    "list-scss": "find src -name '*.scss' | xargs echo",
    "list-tests": "find src -name '*.spec.ts*'",
    "fix-prettier": "prettier --write $(npm run --silent list-ts) $(npm run --silent list-scss)",
    "fix-eslint": "eslint --fix $(npm run --silent list-ts)",
    "\n==================== SERVER ====================": "",
    "server-start": "npm run lint-node && npm run ts-node src/server/index.ts",
    "\n==================== WEB ====================": "",
    "web-start": "npm run lint-node && BROWSER=none react-scripts start",
    "web-build": "npm run lint-node && react-scripts build",
    "web-deploy": "npm run web-build && aws --profile nightbear s3 cp --acl public-read --cache-control=no-store,must-revalidate --recursive build s3://aws-static-site---stage-nightbear-fi/",
    "\n==================== UTILS ====================": "",
    "ts-node": "NODE_PATH=src ts-node --compiler-options '{\"module\":\"commonjs\"}'"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/body-parser": "^1.16.5",
    "@types/chai": "^4.0.4",
    "@types/cli-progress": "^1.8.0",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.37",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/luxon": "^0.5.3",
    "@types/mocha": "^2.2.43",
    "@types/node": "^12.12.24",
    "@types/pouchdb": "6.4.0",
    "@types/pouchdb-find": "6.3.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.4",
    "@types/scroll-into-view": "^1.6.7",
    "axios": "^0.19.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "check-node-version": "^3.1.1",
    "chokidar-cli": "^1.2.0",
    "cli-progress": "^2.1.0",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "debug": "^4.1.1",
    "emotion": "^10.0.27",
    "express": "^4.16.2",
    "husky": "^0.14.3",
    "lodash": "^4.17.15",
    "luxon": "^1.3.0",
    "mocha": "^10.2.0",
    "pouchdb": "7.2.2",
    "pouchdb-adapter-memory": "7.2.2",
    "pouchdb-find": "7.2.2",
    "prettier": "^1.15.3",
    "prettier-check": "^2.0.0",
    "pretty-checkbox": "^3.0.3",
    "pretty-checkbox-react": "^2.0.0-rc.1",
    "pushover-notifications": "^1.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^5.0.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "sass": "^1.49.7",
    "scroll-into-view": "^1.14.1",
    "ts-node": "^3.3.0",
    "typescript": "^3.7.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
