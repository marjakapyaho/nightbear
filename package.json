{
  "name": "nightbear",
  "version": "0.0.1",
  "author": "Marja KÃ¤pyaho <marja.kapyaho@iki.fi>",
  "contributors": [
    "Jarno Rantanen <jarno@jrw.fi>"
  ],
  "license": "AGPL",
  "engines": {
    "node": "18.16.1",
    "npm": "9.5.1"
  },
  "scripts": {
    "\n==================== GLOBAL HOOKS ====================": "",
    "preinstall": "if [ -f ./node_modules/.bin/check-node-version ]; then check-node-version --package; fi",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run lint && npm test",
    "\n==================== GLOBAL QA ====================": "",
    "test": "NODE_PATH=src TS_NODE_PROJECT=tsconfig.mocha.json mocha --require ts-node/register --require $(find src -name PouchDb.mocha.ts) $(npm run --silent list-tests)",
    "lint": "concurrently npm:lint-node npm:lint-tsc npm:lint-eslint npm:lint-prettier",
    "lint-node": "check-node-version --package",
    "lint-tsc": "tsc --project . --noEmit",
    "lint-eslint": "eslint --max-warnings 0 $(npm run --silent list-ts)",
    "lint-prettier": "prettier --list-different $(npm run --silent list-ts) $(npm run --silent list-scss)",
    "list-ts": "find src -name '*.ts*' | grep -v '.snap$' | xargs echo",
    "list-scss": "find src -name '*.scss' | xargs echo",
    "list-tests": "find src -name '*.spec.ts*'",
    "fix-prettier": "prettier --write $(npm run --silent list-ts) $(npm run --silent list-scss)",
    "fix-eslint": "eslint --fix $(npm run --silent list-ts)",
    "\n==================== SERVER ====================": "",
    "server-start": "npm run lint-node && npm run ts-node src/backend/index.ts",
    "\n==================== WEB ====================": "",
    "web-start": "npm run lint-node && BROWSER=none react-scripts start",
    "web-build": "npm run lint-node && react-scripts build",
    "web-deploy": "npm run web-build && aws --profile nightbear s3 cp --acl public-read --cache-control=no-store,must-revalidate --recursive build s3://aws-static-site---stage-nightbear-fi/",
    "\n==================== UTILS ====================": "",
    "ts-node": "NODE_PATH=src ts-node --compiler-options '{\"module\":\"commonjs\"}'"
  },
  "dependencies": {
    "@emotion/css": "11.11.2",
    "@redux-devtools/extension": "3.3.0",
    "@tanstack/react-query": "^5.28.9",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@types/body-parser": "1.19.5",
    "@types/chai": "4.3.11",
    "@types/cli-progress": "3.11.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/lodash": "4.14.202",
    "@types/luxon": "3.3.7",
    "@types/mocha": "10.0.6",
    "@types/node": "20.10.4",
    "@types/pouchdb": "6.4.2",
    "@types/pouchdb-find": "7.3.3",
    "@types/react": "18.2.42",
    "@types/react-dom": "18.2.17",
    "@types/react-redux": "7.1.32",
    "@types/react-router-dom": "5.3.3",
    "@types/scroll-into-view": "1.16.4",
    "axios": "1.6.2",
    "body-parser": "1.20.2",
    "chai": "4.3.10",
    "check-node-version": "4.2.1",
    "chokidar-cli": "3.0.0",
    "cli-progress": "3.12.0",
    "concurrently": "8.2.2",
    "cors": "2.8.5",
    "debug": "4.3.3",
    "express": "4.18.2",
    "husky": "8.0.3",
    "lodash": "4.17.21",
    "luxon": "1.28.1",
    "mocha": "10.2.0",
    "pouchdb": "8.0.1",
    "pouchdb-adapter-memory": "8.0.1",
    "pouchdb-find": "8.0.1",
    "prettier": "3.1.0",
    "prettier-check": "2.0.0",
    "pretty-checkbox": "3.0.3",
    "pretty-checkbox-react": "3.2.0",
    "pushover-notifications": "1.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "9.0.2",
    "react-router-dom": "6.20.1",
    "react-scripts": "5.0.1",
    "redux": "5.0.0",
    "sass": "1.69.5",
    "scroll-into-view": "1.16.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "overrides": {
    "nth-check": "2.1.1",
    "postcss": "8.4.31",
    "typescript": "5.3.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
