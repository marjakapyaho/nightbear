{
  "name": "nightbear",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "engines": {
    "npm": ">=5.4.2 <6",
    "node": ">=8.10.0 <9"
  },
  "scripts": {
    "==================== GLOBAL HOOKS ====================": "",
    "preinstall": "if [ -f ./node_modules/.bin/check-node-version ]; then check-node-version --package; fi",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run lint && npm test",
    "==================== GLOBAL QA ====================": "",
    "lint": "concurrently 'check-node-version --package' 'tsc --noEmit --project .' 'tslint --project .' 'npm run --silent prettier-check'",
    "lint-watch": "chokidar '**/*.{ts,tsx}' --ignore node_modules --debounce 100 --initial --command 'npm run --silent lint'",
    "lint-fix": "npm run prettier-fix",
    "test": "check-node-version --package && NODE_PATH=. mocha --require ts-node/register --require core/storage/PouchDb.mocha $(npm run --silent test-list)",
    "test-watch": "NODE_PATH=. mocha --require ts-node/register --require core/storage/PouchDb.mocha --watch --watch-extensions ts $(npm run --silent test-list)",
    "test-list": "find . -name '*.spec.ts' -not -path '*/node_modules/*'",
    "prettier-check": "prettier-check $(npm run --silent prettier-list)",
    "prettier-fix": "prettier --write $(npm run --silent prettier-list)",
    "prettier-list": "find core server web -name '*.ts' -or -name '*.tsx' -or -name '*.scss'",
    "==================== SERVER ====================": "",
    "server-start": "check-node-version --package && NODE_PATH=. ts-node server/index.ts",
    "==================== WEB ====================": "",
    "web-start": "webpack-dev-server --config web/webpack.config.js --mode development --hot --progress --color --port 3000",
    "web-build": "webpack --config web/webpack.config.js -p --progress --colors",
    "web-deploy": "npm run web-build && aws --profile nightbear s3 cp --acl public-read --cache-control=no-store,must-revalidate --recursive web/dist s3://stage-nightbear-fi/"
  },
  "author": "Marja KÃ¤pyaho <marja.kapyaho@iki.fi>",
  "contributors": [
    "Jarno Rantanen <jarno@jrw.fi>"
  ],
  "license": "AGPL",
  "dependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/chai": "^4.0.4",
    "@types/cli-progress": "^1.8.0",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.37",
    "@types/highcharts": "^5.0.25",
    "@types/lodash": "^4.14.104",
    "@types/luxon": "^0.5.3",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.34",
    "@types/pouchdb": "^6.3.2",
    "@types/pouchdb-find": "^6.3.2",
    "@types/react": "16.0.40",
    "@types/react-dom": "16.0.4",
    "@types/react-redux": "^6.0.1",
    "@types/webpack": "3.8.8",
    "@types/webpack-env": "1.13.5",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "check-node-version": "^3.1.1",
    "chokidar-cli": "^1.2.0",
    "cli-progress": "^2.1.0",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "cors": "^2.8.4",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.10",
    "css-ns": "^1.2.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
