{
  "name": "api",
  "version": "1.0.0",
  "license": "AGPL-3.0",
  "engines": {
    "node": "18.x"
  },
  "main": "index.ts",
  "scripts": {
    "dev": "yarn db:types && yarn ts:node ./api/index.ts",
    "build": "rm -rf build && tsc --project . --outDir build --noEmit false --target es2020 --module commonjs",
    "test": "vitest",
    "lint": "eslint src --ext ts,tsx && tsc",
    "db:reset": "docker-compose stop && docker-compose rm -f && docker-compose up -d && sleep 5 && yarn db:migrate",
    "db:migrate": "contrib/db/migrate test && contrib/db/migrate dev",
    "db:types": "pgtyped --config pgtyped-config.json",
    "ts:node": "ts-node --compiler-options '{\"module\":\"commonjs\"}'"
  },
  "lambdaDependencies": [
    "@pgtyped/runtime",
    "axios",
    "body-parser",
    "check-node-version",
    "cors",
    "debug",
    "express",
    "lodash",
    "luxon",
    "pg",
    "pushover-notifications",
    "zod"
  ],
  "dependencies": {
    "@nightbear/shared": "file:../shared",
    "@pgtyped/cli": "2.3.0",
    "@pgtyped/runtime": "2.3.0",
    "@types/body-parser": "1.19.5",
    "@types/cli-progress": "3.11.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/lodash": "4.14.202",
    "@types/luxon": "3.3.7",
    "@types/node": "20.10.4",
    "axios": "1.6.2",
    "body-parser": "1.20.2",
    "check-node-version": "4.2.1",
    "chokidar-cli": "3.0.0",
    "cli-progress": "3.12.0",
    "cors": "2.8.5",
    "debug": "4.3.4",
    "express": "4.19.2",
    "lodash": "4.17.21",
    "luxon": "1.28.1",
    "ml-regression-simple-linear": "^3.0.0",
    "node-pg-migrate": "6.2.2",
    "pg": "8.11.4",
    "prettier": "3.1.0",
    "prettier-check": "2.0.0",
    "pushover-notifications": "1.2.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "zod": "3.23.6"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "vitest": "^1.5.0"
  },
  "overrides": {
    "nth-check": "2.1.1",
    "typescript": "5.3.3"
  }
}
